{
  "name": "fatsecret.js",
  "version": "1.0.6",
  "description": "Fatsecret.js is an unofficial API wrapper for the FatSecret nutrition database.",
  "main": "./lib/main.js",
  "types": "./lib/main.d.ts",
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "dotenv": "^16.0.2",
    "jest": "^29.0.3",
    "jsdoc": "^3.6.11",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IDontLua/fatsecret.js.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IDontLua/fatsecret.js/issues"
  },
  "homepage": "https://github.com/IDontLua/fatsecret.js#readme",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "mathjs": "^11.2.1"
  }
}
